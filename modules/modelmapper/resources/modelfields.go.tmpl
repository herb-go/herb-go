	//Skip field "{{.Field}}" which should be set by database
	//Field  "{{$Name.Raw }}.{{.Field}}"
	//{{$Name.Pascal}}Field{{.Name}},
	{{- else }}
	//Field  "{{$Name.Raw }}.{{.Field}}"
	{{$Name.Pascal}}Field{{.Name}},
	{{- end }}
	{{- end}}
}

